	if(!IsPlayerInRangeOfPoint(playerid, 1.0, -80.710136, -1193.329956, 2.947619)) return ShowPlayerMessage(playerid, "No estás en el lugar adecuado.", 3);
	
	if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, -1, "Error: {C4FF66}/piezas {FFFFFF}[cantidad]");
	if(params[0] <= 0 || params[0] >= 10000) return SendClientMessage(playerid, -1, "Error: {C4FF66}/piezas {FFFFFF}[cantidad > 0]");
	
