				PLAYER_TEMP[playerid][pt_INJURED_POS][2] = CHARACTER_INFO[playerid][ch_POS][2];
				PLAYER_TEMP[playerid][pt_INJURED_POS][3] = CHARACTER_INFO[playerid][ch_ANGLE];
				
