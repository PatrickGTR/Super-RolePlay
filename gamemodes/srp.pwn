		db_free_result(Result_vobjects);
		
		SetVehicleToRespawnEx(vehicle_id);

		
		
		new max_slots = VEHICLE_INFO[GLOBAL_VEHICLES[vehicle_id][gb_vehicle_MODELID] - 400][vehicle_info_BOOT_SLOTS];
